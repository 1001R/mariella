<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
	<persistence-unit name="test">
		
		<property name="org.mariella.oxygen.persistenceBuilder" value="org.mariella.persistence.h2.H2PersistenceBuilder"/>
		<property name="org.mariella.persistence.annotations.mapping_builder.DatabaseMetaDataDatabaseInfoProvider.ignoreSchema" value="true"/>
		<property name="org.mariella.persistence.annotations.mapping_builder.DatabaseMetaDataDatabaseInfoProvider.ignoreCatalog" value="true"/>
		
		<property name="org.mariella.oxygen.jdbcdriver" value="org.h2.Driver"/>
		<property name="org.mariella.oxygen.connectstring" value="jdbc:h2:~/test"/>
		<property name="org.mariella.oxygen.dbuser" value=""/>
		<property name="org.mariella.oxygen.dbpassword" value=""/>
	</persistence-unit>
</persistence>